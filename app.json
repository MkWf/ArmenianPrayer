{
  "expo": {
    "name": "ArmenianReader",
    "slug": "ArmenianReader",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/asset/icon.png",
    "splash": {
      "image": "./src/asset/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "asset/**",
      "src/library/assets/**/*",
      "src/library/databases/*",
      "src/library/fonts/*",
      "src/asset/*"
    ],
    "packagerOpts": {
      "assetExts": ["db"]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/asset/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/asset/favicon.png"
    }
  }
}
